<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>local.seaflow-realtime-heartbeat</string>
        <key>ProgramArguments</key>
        <array>
            <string>/Users/seaflow/realtime/bin/seaflow-realtime-heartbeat</string>
            <string>/Users/seaflow/realtime/seaflow-realtime.conf</string>
        </array>
        <key>RunAtLoad</key>
        <true/>
        <key>StartInterval</key>
        <integer>600</integer>
        <key>ThrottleInterval</key>
        <integer>30</integer>
        <key>UserName</key>
        <string>seaflow</string>
        <key>GroupName</key>
        <string>staff</string>
        <key>StandardOutPath</key>
        <string>/Users/seaflow/realtime/log/seaflow-realtime-heartbeat.stdout</string>
        <key>StandardErrorPath</key>
        <string>/Users/seaflow/realtime/log/seaflow-realtime-heartbeat.stderr</string>
        <key>WorkingDirectory</key>
        <string>/Users/seaflow</string>
    </dict>
</plist>
